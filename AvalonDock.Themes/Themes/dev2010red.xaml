											<Border BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="1"
											        CornerRadius="1" />
										</Grid>

									</Border>
									<Border Name="PART_btnDockPaneLeft"
									        Grid.Row="1"
									        Grid.Column="0"
									        Margin="2"
									        Background="{DynamicResource OverlayWindowIntBorderBackground}"
									        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush}"
									        BorderThickness="1"
									        CornerRadius="2">
										<Grid Margin="2">
											<Rectangle Margin="0,0,12,0"
											           Fill="{DynamicResource OverlayWindowIntBorderBackground2}"
											           Stroke="{DynamicResource OverlayWindowIntBorderBrush2}"
											           StrokeDashCap="Square">
												<Rectangle.StrokeDashArray>
													<sys:Double>2</sys:Double>
												</Rectangle.StrokeDashArray>
											</Rectangle>
											<Border Height="3"
											        VerticalAlignment="Top"
											        Background="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="0,0,0,1" />
											<Border BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="1"
											        CornerRadius="1" />
										</Grid>

									</Border>
									<Border Name="PART_btnDockPaneInto"
									        Grid.Row="1"
									        Grid.Column="1"
									        Margin="2"
									        Background="{DynamicResource OverlayWindowIntBorderBackground}"
									        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush}"
									        BorderThickness="1"
									        CornerRadius="2">
										<Grid Margin="2">
											<Border Background="{DynamicResource OverlayWindowIntBorderBackground2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="1"
											        CornerRadius="1" />
											<Border Height="3"
											        VerticalAlignment="Top"
											        Background="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="0,0,0,1" />
										</Grid>
									</Border>
								</Grid>
							</Grid>
						</Canvas>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!--  DockableFloatingWindow  -->
	<Style x:Key="{x:Type ad:DockableFloatingWindow}" TargetType="{x:Type ad:DockableFloatingWindow}">
		<Setter Property="ResizeMode" Value="NoResize" />
		<Setter Property="WindowStyle" Value="None" />
		<Setter Property="Background" Value="{x:Null}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type ad:DockableFloatingWindow}">
					<Border x:Name="ExtBorder"
					        Background="#7A4935"
					        CornerRadius="2">
						<Grid>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="5" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="5" />
							</Grid.ColumnDefinitions>
							<Grid.RowDefinitions>
								<RowDefinition Height="5" />
								<RowDefinition Height="20" />
								<RowDefinition Height="*" />
								<RowDefinition Height="5" />
							</Grid.RowDefinitions>

							<ad:Resizer x:Name="PART_LeftAnchor"
							            Grid.Row="1"
							            Grid.RowSpan="2"
							            Grid.Column="0"
							            Width="4"
							            VerticalAlignment="Stretch"
							            Cursor="SizeWE" />
							<ad:Resizer x:Name="PART_RightAnchor"
							            Grid.Row="1"
							            Grid.RowSpan="2"
							            Grid.Column="2"
							            Width="4"
							            VerticalAlignment="Stretch"
							            Cursor="SizeWE" />
							<ad:Resizer x:Name="PART_TopAnchor"
							            Grid.Row="0"
							            Grid.Column="1"
							            Height="4"
							            HorizontalAlignment="Stretch"
							            Cursor="SizeNS" />
							<ad:Resizer x:Name="PART_BottomAnchor"
							            Grid.Row="3"
							            Grid.Column="1"
							            Height="4"
							            HorizontalAlignment="Stretch"
							            Cursor="SizeNS" />

							<ad:Resizer x:Name="PART_LeftTopAnchor"
							            Grid.Row="0"
							            Grid.Column="0"
							            Width="4"
							            Height="4"
							            HorizontalAlignment="Stretch"
							            VerticalAlignment="Stretch"
							            Cursor="SizeNWSE" />
							<ad:Resizer x:Name="PART_LeftBottomAnchor"
							            Grid.Row="3"
							            Grid.Column="0"
							            Width="4"
							            Height="4"
							            HorizontalAlignment="Stretch"
							            VerticalAlignment="Stretch"
							            Cursor="SizeNESW" />
							<ad:Resizer x:Name="PART_RightTopAnchor"
							            Grid.Row="0"
							            Grid.Column="2"
							            Width="4"
							            Height="4"
							            HorizontalAlignment="Stretch"
							            VerticalAlignment="Stretch"
							            Cursor="SizeNESW" />
							<ad:Resizer x:Name="PART_RightBottomAnchor"
							            Grid.Row="3"
							            Grid.Column="3"
							            Width="4"
							            Height="4"
							            HorizontalAlignment="Stretch"
							            VerticalAlignment="Stretch"
							            Cursor="SizeNWSE" />

							<Border x:Name="PART_MoveAnchor"
							        Grid.Row="1"
							        Grid.Column="1"
							        Background="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager},
							                                                           {x:Static ad:AvalonDockBrushes.DockablePaneTitleBackground}}}"
							        CornerRadius="2,2,0,0">
								<Grid x:Name="intTitleGrid">
									<Grid.ColumnDefinitions>
										<ColumnDefinition Width="*" />
										<ColumnDefinition Width="16" />
										<ColumnDefinition Width="16" />
									</Grid.ColumnDefinitions>
									<TextBlock x:Name="intTitle"
									           Margin="2,0,0,0"
									           HorizontalAlignment="Stretch"
									           VerticalAlignment="Center"
									           Foreground="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager},
									                                                              {x:Static ad:AvalonDockBrushes.DockablePaneTitleForeground}}}"
									           IsHitTestVisible="False"
									           Text="{TemplateBinding Title}" />
									<Border x:Name="PART_ShowContextMenuButton"
									        Grid.Column="1"
									        Width="15"
									        Height="15"
									        HorizontalAlignment="Center"
									        VerticalAlignment="Center"
									        Opacity="0.6"
									        Style="{StaticResource PaneHeaderContextMenuCommandStyle}">
										<Image Width="13"
										       Height="13"
										       HorizontalAlignment="Center"
										       VerticalAlignment="Center"
										       Source="/AvalonDock;component/resources/Images/PinMenu.png"
										       Stretch="None" />
									</Border>
									<Button x:Name="PART_CloseButton"
									        Grid.Column="2"
									        Width="15"
									        Height="15"
									        HorizontalAlignment="Center"
									        VerticalAlignment="Center"
									        Command="ad:DockablePaneCommands.Close"
									        CommandTarget="{TemplateBinding Content}"
									        Opacity="0.6"
									        Style="{StaticResource PaneHeaderCommandStyle}">
										<Image Width="13"
										       Height="13"
										       HorizontalAlignment="Center"
										       VerticalAlignment="Center"
										       Source="/AvalonDock;component/resources/Images/PinClose.png"
										       Stretch="None" />
									</Button>
								</Grid>
							</Border>
							<Border Grid.Row="2"
							        Grid.Column="1"
							        Background="{TemplateBinding Background}">
								<ContentPresenter />
							</Border>
						</Grid>
					</Border>
					<ControlTemplate.Triggers>
						<DataTrigger Binding="{Binding RelativeSource={x:Static RelativeSource.Self}, Path=Content.ContainsActiveContent}" Value="True">
							<Setter TargetName="PART_MoveAnchor" Property="Background" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DockablePaneTitleBackgroundSelected}}}" />
							<Setter TargetName="intTitle" Property="Foreground" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DockablePaneTitleForegroundSelected}}}" />
						</DataTrigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<!--  DocumentFloatingWindowExtBorderSelectedBrush  -->
	<SolidColorBrush x:Key="DocumentFloatingWindowExtBorderSelectedBrush" Color="#C0A776" />

	<!--  DocumentFloatingWindow  -->
	<Style x:Key="{x:Type ad:DocumentFloatingWindow}" TargetType="{x:Type ad:DocumentFloatingWindow}">
		<Setter Property="ResizeMode" Value="NoResize" />
		<Setter Property="WindowStyle" Value="None" />
		<Setter Property="Background" Value="{x:Null}" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type ad:DocumentFloatingWindow}">
					<Border x:Name="ExtBorder"
					        BorderBrush="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager},
					                                                            {x:Static ad:AvalonDockBrushes.DocumentHeaderBorder}}}"
					        BorderThickness="1">
						<Grid>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="5" />
								<ColumnDefinition Width="*" />
								<ColumnDefinition Width="5" />
							</Grid.ColumnDefinitions>
							<Grid.RowDefinitions>
								<RowDefinition Height="5" />
								<RowDefinition Height="20" />
								<RowDefinition Height="*" />
								<RowDefinition Height="5" />
							</Grid.RowDefinitions>

							<Border x:Name="PART_MoveAnchor"
							        Grid.Row="0"
							        Grid.RowSpan="2"
							        Grid.Column="0"
							        Grid.ColumnSpan="3"
							        Background="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager},
							                                                           {x:Static ad:AvalonDockBrushes.DocumentHeaderBorder}}}">
								<Grid x:Name="intTitleGrid">
									<Grid.ColumnDefinitions>
										<ColumnDefinition Width="*" />
										<ColumnDefinition Width="16" />
										<ColumnDefinition Width="16" />
									</Grid.ColumnDefinitions>
									<TextBlock x:Name="intTitle"
									           Margin="2,0,0,0"
									           HorizontalAlignment="Stretch"
									           VerticalAlignment="Center"
									           Foreground="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager},
									                                                              {x:Static ad:AvalonDockBrushes.DocumentHeaderForeground}}}"
									           IsHitTestVisible="False"
									           Text="{TemplateBinding Title}" />
									<Border x:Name="PART_ShowContextMenuButton"
									        Grid.Column="1"
									        Width="15"
									        Height="15"
									        HorizontalAlignment="Center"
									        VerticalAlignment="Center"
									        Opacity="0.6"
									        Style="{StaticResource PaneHeaderContextMenuCommandStyle}">
										<Image Width="13"
										       Height="13"
										       HorizontalAlignment="Center"
										       VerticalAlignment="Center"
										       Source="/AvalonDock;component/resources/Images/PinMenu.png"
										       Stretch="None" />
									</Border>
									<Button x:Name="PART_CloseButton"
									        Grid.Column="2"
									        Width="15"
									        Height="15"
									        HorizontalAlignment="Center"
									        VerticalAlignment="Center"
									        Command="ad:DocumentPaneCommands.CloseThis"
									        CommandTarget="{TemplateBinding Content}"
									        Opacity="0.6"
									        Style="{StaticResource PaneHeaderCommandStyle}">
										<Image Width="13"
										       Height="13"
										       HorizontalAlignment="Center"
										       VerticalAlignment="Center"
										       Source="/AvalonDock;component/resources/Images/PinClose.png"
										       Stretch="None" />
									</Button>
								</Grid>
							</Border>

							<ContentPresenter Grid.Row="2"
							                  Grid.RowSpan="2"
							                  Grid.Column="0"
							                  Grid.ColumnSpan="3"
							                  Margin="4"
							                  Content="{Binding RelativeSource={RelativeSource TemplatedParent},
							                                    Path=HostedPane.SelectedItem.Content}" />
							<Border x:Name="intContentBorder"
							        Grid.Row="2"
							        Grid.RowSpan="2"
							        Grid.Column="0"
							        Grid.ColumnSpan="3"
							        Background="{TemplateBinding Background}"
							        BorderBrush="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager},
							                                                            {x:Static ad:AvalonDockBrushes.DocumentHeaderBorder}}}"
							        BorderThickness="4" />

							<ad:Resizer x:Name="PART_LeftAnchor"
							            Grid.Row="1"
							            Grid.RowSpan="2"
							            Grid.Column="0"
							            Width="4"
							            VerticalAlignment="Stretch"
							            Cursor="SizeWE" />
							<ad:Resizer x:Name="PART_RightAnchor"
							            Grid.Row="1"
							            Grid.RowSpan="2"
							            Grid.Column="2"
							            Width="4"
							            VerticalAlignment="Stretch"
							            Cursor="SizeWE" />
							<ad:Resizer x:Name="PART_TopAnchor"
							            Grid.Row="0"
							            Grid.Column="1"
							            Height="4"
							            HorizontalAlignment="Stretch"
							            Cursor="SizeNS" />
							<ad:Resizer x:Name="PART_BottomAnchor"
							            Grid.Row="3"
							            Grid.Column="1"
							            Height="4"
							            HorizontalAlignment="Stretch"
							            Cursor="SizeNS" />

							<ad:Resizer x:Name="PART_LeftTopAnchor"
							            Grid.Row="0"
							            Grid.Column="0"
							            Width="4"
							            Height="4"
							            HorizontalAlignment="Stretch"
							            VerticalAlignment="Stretch"
							            Cursor="SizeNWSE" />
							<ad:Resizer x:Name="PART_LeftBottomAnchor"
							            Grid.Row="3"
							            Grid.Column="0"
							            Width="4"
							            Height="4"
							            HorizontalAlignment="Stretch"
							            VerticalAlignment="Stretch"
							            Cursor="SizeNESW" />
							<ad:Resizer x:Name="PART_RightTopAnchor"
							            Grid.Row="0"
							            Grid.Column="2"
							            Width="4"
							            Height="4"
							            HorizontalAlignment="Stretch"
							            VerticalAlignment="Stretch"
							            Cursor="SizeNESW" />
							<ad:Resizer x:Name="PART_RightBottomAnchor"
							            Grid.Row="3"
							            Grid.Column="3"
							            Width="4"
							            Height="4"
							            HorizontalAlignment="Stretch"
							            VerticalAlignment="Stretch"
							            Cursor="SizeNWSE" />
						</Grid>
					</Border>
					<ControlTemplate.Triggers>
						<DataTrigger Binding="{Binding RelativeSource={x:Static RelativeSource.Self}, Path=HostedPane.ContainsActiveDocument}" Value="True">
							<Setter TargetName="PART_MoveAnchor" Property="Background" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderBackgroundSelected}}}" />
							<Setter TargetName="intContentBorder" Property="BorderBrush" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderBorderSelected}}}" />
							<Setter TargetName="intTitle" Property="TextBlock.Foreground" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderForegroundSelected}}}" />
							<Setter TargetName="ExtBorder" Property="BorderBrush" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderBorderSelected}}}" />
						</DataTrigger>
						<DataTrigger Binding="{Binding RelativeSource={x:Static RelativeSource.Self}, Path=HostedPane.ContainsActiveContent}" Value="True">
							<Setter TargetName="PART_MoveAnchor" Property="Background" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderBackgroundSelectedActivated}}}" />
							<Setter TargetName="intContentBorder" Property="BorderBrush" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderBorderSelectedActivated}}}" />
							<Setter TargetName="intTitle" Property="TextBlock.Foreground" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderForegroundSelectedActivated}}}" />
							<Setter TargetName="ExtBorder" Property="BorderBrush" Value="{DynamicResource DocumentFloatingWindowExtBorderSelectedBrush}" />
						</DataTrigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
</ResourceDictionary>
