		</LinearGradientBrush.GradientStops>
	</LinearGradientBrush>

	<!--  OverlayWindow  -->
	<Style x:Key="{x:Type ad:OverlayWindow}" TargetType="{x:Type ad:OverlayWindow}">
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="Focusable" Value="False" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type ad:OverlayWindow}">
					<Grid Opacity="0.9" SnapsToDevicePixels="True">
						<Canvas>
							<Border x:Name="PART_SelectionBox"
							        Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}"
							        BorderBrush="Gray"
							        BorderThickness="3"
							        Opacity="0.2"
							        Visibility="Hidden" />
						</Canvas>
						<Grid x:Name="panelDrag">
							<Border Name="PART_btnDockRight"
							        Width="35"
							        Height="35"
							        HorizontalAlignment="Right"
							        VerticalAlignment="Center"
							        BorderBrush="{DynamicResource OverlayWindowMainBorderBrush}"
							        BorderThickness="1">
								<Grid>
									<Border Background="White" Opacity="0.3" />
									<Border Margin="2"
									        Background="{DynamicResource OverlayWindowIntBorderBackground}"
									        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush}"
									        BorderThickness="1"
									        CornerRadius="2">
										<Grid Margin="2">
											<Border Margin="10,0,0,0"
											        Background="{DynamicResource OverlayWindowIntBorderBackground2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="1"
											        CornerRadius="1" />
											<Border Height="3"
											        Margin="10,0,0,0"
											        VerticalAlignment="Top"
											        Background="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="0,0,0,1" />
											<Path Margin="2,0,0,0"
											      HorizontalAlignment="Left"
											      VerticalAlignment="Center"
											      Data="M0,0 4,4 0,8 z"
											      Fill="{DynamicResource OverlayWindowIntBorderBrush2}"
											      StrokeThickness="0" />
										</Grid>
									</Border>
								</Grid>
							</Border>

							<Border Name="PART_btnDockLeft"
							        Width="35"
							        Height="35"
							        HorizontalAlignment="Left"
							        VerticalAlignment="Center"
							        BorderBrush="{DynamicResource OverlayWindowMainBorderBrush}"
							        BorderThickness="1">
								<Grid>
									<Border Background="White" Opacity="0.3" />
									<Border Margin="2"
									        Background="{DynamicResource OverlayWindowIntBorderBackground}"
									        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush}"
									        BorderThickness="1"
									        CornerRadius="2">
										<Grid Margin="2">
											<Border Margin="0,0,10,0"
											        Background="{DynamicResource OverlayWindowIntBorderBackground2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="1"
											        CornerRadius="1" />
											<Border Height="3"
											        Margin="0,0,10,0"
											        VerticalAlignment="Top"
											        Background="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="0,0,0,1" />
											<Path Margin="0,0,2,0"
											      HorizontalAlignment="Right"
											      VerticalAlignment="Center"
											      Data="M0,0 -4,4 0,8 z"
											      Fill="{DynamicResource OverlayWindowIntBorderBrush2}"
											      StrokeThickness="0" />
										</Grid>
									</Border>
								</Grid>
							</Border>

							<Border Name="PART_btnDockTop"
							        Width="35"
							        Height="35"
							        HorizontalAlignment="Center"
							        VerticalAlignment="Top"
							        BorderBrush="{DynamicResource OverlayWindowMainBorderBrush}"
							        BorderThickness="1">
								<Grid>
									<Border Background="White" Opacity="0.3" />
									<Border Margin="2"
									        Background="{DynamicResource OverlayWindowIntBorderBackground}"
									        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush}"
									        BorderThickness="1"
									        CornerRadius="2">
										<Grid Margin="2">
											<Border Margin="0,0,0,10"
											        Background="{DynamicResource OverlayWindowIntBorderBackground2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="1"
											        CornerRadius="1" />
											<Border Height="3"
											        Margin="0,0,0,10"
											        VerticalAlignment="Top"
											        Background="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="0,0,0,1" />
											<Path Margin="0,0,0,2"
											      HorizontalAlignment="Center"
											      VerticalAlignment="Bottom"
											      Data="M0,0 4,-4 8,0 z"
											      Fill="{DynamicResource OverlayWindowIntBorderBrush2}"
											      StrokeThickness="0" />
										</Grid>
									</Border>
								</Grid>
							</Border>

							<Border Name="PART_btnDockBottom"
							        Width="35"
							        Height="35"
							        HorizontalAlignment="Center"
							        VerticalAlignment="Bottom"
							        BorderBrush="{DynamicResource OverlayWindowMainBorderBrush}"
							        BorderThickness="1">
								<Grid>
									<Border Background="White" Opacity="0.3" />
									<Border Margin="2"
									        Background="{DynamicResource OverlayWindowIntBorderBackground}"
									        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush}"
									        BorderThickness="1"
									        CornerRadius="2">
										<Grid Margin="2">
											<Border Margin="0,10,0,0"
											        Background="{DynamicResource OverlayWindowIntBorderBackground2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="1"
											        CornerRadius="1" />
											<Border Height="3"
											        Margin="0,10,0,0"
											        VerticalAlignment="Top"
											        Background="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="0,0,0,1" />
											<Path Margin="0,2,0,0"
											      HorizontalAlignment="Center"
											      VerticalAlignment="Top"
											      Data="M0,0 4,4 8,0 z"
											      Fill="{DynamicResource OverlayWindowIntBorderBrush2}"
											      StrokeThickness="0" />
										</Grid>
									</Border>
								</Grid>
							</Border>
						</Grid>
						<Canvas>
							<Grid Name="PART_gridPaneRelativeDockingOptions" Visibility="Collapsed">
								<Grid Width="100"
								      Height="100"
								      HorizontalAlignment="Center"
								      VerticalAlignment="Center">
									<Grid.ColumnDefinitions>
										<ColumnDefinition />
										<ColumnDefinition />
										<ColumnDefinition />
									</Grid.ColumnDefinitions>
									<Grid.RowDefinitions>
										<RowDefinition />
										<RowDefinition />
										<RowDefinition />
									</Grid.RowDefinitions>
									<Path Grid.RowSpan="3"
									      Grid.ColumnSpan="3"
									      Data="M25,0 55,0 55,20 60,25 80,25 80,55, 60,55 55,60 55,80 25,80 25,60 20,55 0,55 0,25 20,25 25,20 z"
									      Fill="White"
									      Opacity="0.3"
									      Stretch="Fill"
									      Stroke="{DynamicResource OverlayWindowMainBorderBrush}" />
									<Border Name="PART_btnDockPaneTop"
									        Grid.Row="0"
									        Grid.Column="1"
									        Margin="2"
									        Background="{DynamicResource OverlayWindowIntBorderBackground}"
									        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush}"
									        BorderThickness="1"
									        CornerRadius="2">
										<Grid Margin="2">
											<Rectangle Margin="0,0,0,12"
											           Fill="{DynamicResource OverlayWindowIntBorderBackground2}"
											           Stroke="{DynamicResource OverlayWindowIntBorderBrush2}"
											           StrokeDashCap="Square">
												<Rectangle.StrokeDashArray>
													<sys:Double>2</sys:Double>
												</Rectangle.StrokeDashArray>
											</Rectangle>
											<Border Height="3"
											        VerticalAlignment="Top"
											        Background="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="0,0,0,1" />
											<Border BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="1"
											        CornerRadius="1" />
										</Grid>
									</Border>
									<Border Name="PART_btnDockPaneRight"
									        Grid.Row="1"
									        Grid.Column="2"
									        Margin="2"
									        Background="{DynamicResource OverlayWindowIntBorderBackground}"
									        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush}"
									        BorderThickness="1"
									        CornerRadius="2">
										<Grid Margin="2">
											<Rectangle Margin="12,0,0,0"
											           Fill="{DynamicResource OverlayWindowIntBorderBackground2}"
											           Stroke="{DynamicResource OverlayWindowIntBorderBrush2}"
											           StrokeDashCap="Square">
												<Rectangle.StrokeDashArray>
													<sys:Double>2</sys:Double>
												</Rectangle.StrokeDashArray>
											</Rectangle>
											<Border Height="3"
											        VerticalAlignment="Top"
											        Background="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="0,0,0,1" />
											<Border BorderBrush="{DynamicResource OverlayWindowIntBorderBrush2}"
											        BorderThickness="1"
											        CornerRadius="1" />
										</Grid>

									</Border>
									<Border Name="PART_btnDockPaneBottom"
									        Grid.Row="2"
									        Grid.Column="1"
									        Margin="2"
									        Background="{DynamicResource OverlayWindowIntBorderBackground}"
									        BorderBrush="{DynamicResource OverlayWindowIntBorderBrush}"
									        BorderThickness="1"
									        CornerRadius="2">
										<Grid Margin="2">
											<Rectangle Margin="0,12,0,0"
											           Fill="{DynamicResource OverlayWindowIntBorderBackground2}"
											           Stroke="{DynamicResource OverlayWindowIntBorderBrush2}"
											           StrokeDashCap="Square">
												<Rectangle.StrokeDashArray>
													<sys:Double>2</sys:Double>
												</Rectangle.StrokeDashArray>
											</Rectangle>
											<Border Height="3"
											        VerticalAlignment="Top"
											        Background="{DynamicResource OverlayWindowIntBorderBrush2}"
